{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "TuneInTruth",
    "description": "This is a Chrome extension built with React and TypeScript",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "js/index.html",
        "default_title": "React Chrome Extension",
        "default_icon": "icon.png"
    },
    "permissions": ["storage"]
}